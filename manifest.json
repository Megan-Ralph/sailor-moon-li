{
  "manifest_version": 2,
  "name": "Sailor Moon Lorem Ipsum",
  "version": 1.0,
  "description": "Generates Sailor Moon-themed Lorem Ipsum text",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls"],
      "js": ["content.js"]
    }
  ]
}
